[user]
	email = me@dustinschie.com
	name = Dustin Schie
[push]
	default = matching
[alias]
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	ctags = !.git/hooks/ctags
[hub]
	protocol = https
[core]
	excludesfile = ~/.gitignore_global
[init]
	templatedir = ~/.git_template
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[credential]
	helper = osxkeychain
